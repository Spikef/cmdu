#!/usr/bin/env node

var app = require('../');

app.version = '0.0.1';

app
    .command('* <cmd> [env]')
    .describe('cmd', 'Can be init, install, uninstall, publish')
    .option('-h, --help', 'output the help information')
    .option('-v, --version', 'output the version number')
    .action(function (cmd, env) {
        console.log(cmd);
        console.log(env);
    });

app.listen();
